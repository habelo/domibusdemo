<jaxb:bindings version="2.0"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xsd="http://www.w3.org/2001/XMLSchema"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <jaxb:globalBindings optionalProperty="primitive">
        <jaxb:serializable uid="1"/>
    </jaxb:globalBindings>

    <!-- XSD types packages customization -->
    <jaxb:bindings schemaLocation="./core_components/IntegratedStandardBusinessDocument.xsd" node="/xsd:schema">
        <jaxb:schemaBindings>
            <jaxb:package name="se.inera.sdk.message"/>
        </jaxb:schemaBindings>
    </jaxb:bindings>


</jaxb:bindings>